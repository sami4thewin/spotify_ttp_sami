<%= form_for @person do |f| %>
    <div class="field">
      <%= f.label :name %><br>
      <%= f.text_field :name %>
    </div>
    <div class="field">
      <%= f.label :favoriteCity %><br>
      <%= f.text_field :favoriteCity %>
    </div>
    <div class="actions">
      <%= f.submit 'Update your info'%>
    </div>
<% end %>
